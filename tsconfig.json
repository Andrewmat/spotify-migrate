{
  "compilerOptions": {
    "baseUrl": "src",
    "allowJs": true,
    "jsx": "react",
    "paths": {
      "@/*": ["./*"],
      "@Type": ["./d.ts/index.d.ts"],
      "@Type/*": ["./d.ts/*"]
    },
    "plugins": [{
      "name": "typescript-styled-plugin",
      "lint": {
        "validProperties": [
          "content-visibility",
          "contain-intrinsic-size"
        ]
      }
    }]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
